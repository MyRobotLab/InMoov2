<category><pattern># NAPOVED</pattern>
<template>.<oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny()</param>
        </mrl>
      </oob></template>
</category>
<category><pattern>KAKŠNA JE NAPOVED</pattern>
<template><srai># NAPOVED</srai></template>
</category>
<category><pattern># NAPOVED # $JUTRI</pattern>
<template>.<oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny(town,8)</param>
        </mrl>
      </oob></template>
</category>
<category><pattern># NAPOVED # IN * $DNI</pattern>
<template>.<think><set name="nbdays"><map name="name2number"><star index="3"/></map></set></think>
        <condition>
            <li name="nbdays" value="unknown"><think><set name="nbdays"><star index="3"/></set></think></li>
        </condition>
       <oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny(town,<javascript>
period = eval( <get name="nbdays"/> * 8 );
period;
</javascript>)</param>
        </mrl>
      </oob></template>
</category>
<category><pattern># NAPOVED $IN * $IN * $DNI</pattern>
<template>.<think><set name="nbdays"><map name="name2number"><star index="3"/></map></set></think>
        <condition>
            <li name="nbdays" value="unknown"><think><set name="nbdays"><star index="3"/></set></think></li>
        </condition>
       <oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny("<star index="2"/>",<javascript>
period = eval( <get name="nbdays"/> * 8 );
period;
</javascript>)</param>
        </mrl>
      </oob></template>
</category>
<category><pattern># NAPOVED $IN *</pattern>
<template>.<oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>isTheSunShiny("<star index="2" />",1)</param>
        </mrl>
      </oob></template>
</category>
