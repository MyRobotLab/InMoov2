  <category>
    <pattern> * MORCEAU SUIVANT</pattern>
    <template>
      <think>
        <get name="playsong">
        </get>
      </think>
      <condition name="playsong" value="ok">
        <oob>
          <mrl>
            <service>python</service>
            <method>exec</method>
            <param>nextPlay()</param>
          </mrl>
        </oob>
      </condition>
      <condition name="playsong" value="ko">
        <oob>
          <mrl>
            <service>python</service>
            <method>exec</method>
            <param>i01_audioPlayer.stop()</param>
          </mrl>
        </oob>
      </condition>
    </template>
  </category>
