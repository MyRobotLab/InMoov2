  <category>
    <pattern>CHERCHE LE MORCEAU *</pattern>
    <template>
      <think>
        <set name="playsong">ok</set>
        <set name="song">
          <star />
        </set>
      </think>
      <oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>searchPlay("<get name="song" />")</param>
        </mrl>
      </oob>
    </template>
  </category> 
