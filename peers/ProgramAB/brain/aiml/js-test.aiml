<?xml version="1.0" encoding="UTF-8"?>
<aiml>
  <category>
    <pattern>WHAT IS THE WEATHER LIKE IN <set name="city" />?</pattern>
    <template>
      <javascript>
        function getWeather(city) {
        // Call a weather API to get the weather for the specified city
        // and return the result as a string.
        return "The weather in " + city + " is sunny today.";
        }

        // Get the value of the "city" set from the input pattern.
        var city = AIMLTag("set", "name=city");

        // Call the getWeather function with the city value and return the result.
        return getWeather(city);
      </javascript>
    </template>
  </category>

  <category>
    <pattern>FIND X</pattern>
