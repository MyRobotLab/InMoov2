    <template>
      <random>
        <li>நான் இசையை பிளேலிஸ்ட் பயன்முறையில் வைத்தேன்.</li>
        <li>சரி! நான் பிளேலிஸ்ட் பயன்முறையில் இருக்கிறேன்.</li>
      </random>
      <think>
        <set name="playsong">ko</set>
      </think>
      <oob>
        <mrl>
          <service>python</service>
          <method>exec</method>
          <param>i01_audioPlayer.startPlaylist()</param>
        </mrl>
      </oob>
    </template>
  </category>
  <category>
